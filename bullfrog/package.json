{
  "name": "@asmov/bullfrog",
  "version": "2021.1.1",
  "description": "A library for creating bash command-line toolkits.",
  "main": "dist/bullfrog/bash/bin/bullfrog.bash",
  "bullfrog": {},
  "scripts": {
    "lint": "./tool/bash/bin/lint.bash",
    "compile": "./tool/bash/bin/compile.bash",
    "build": "./tool/bash/bin/build.bash",
    "build.all": "npm run lint && npm run compile && npm run build",
    "test": "mocha --recursive test/js",
    "test.debug": "mocha --allow-uncaught --recursive test/js"
  },
  "files": [
    "dist/bullfrog/bash",
    "dist/bullfrog/config",
    "dist/bullfrog/json",
    "dist/bullfrog/files"
  ],
  "bin": {
    "bullfrog": "dist/bullfrog/bash/bin/bullfrog.bash"
  },
  "exports": {
    "./*": "./js/lib/asmov/bullfrog/*.mjs"
  },
  "preferGlobal": true,
  "keywords": [
    "bullfrog",
    "frog",
    "bash",
    "toolkit",
    "library",
    "automation",
    "scripting",
    "vm",
    "cloud",
    "stack"
  ],
  "author": "Roy Laurie <roy.laurie@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "ajv": "^8.6.1"
  },
  "devDependencies": {
    "ajv-cli": "^5.0.0",
    "chai": "^4.3.4",
    "inspect": "^0.0.2",
    "mocha": "^9.0.3",
    "shlex": "^2.1.0"
  }
}
